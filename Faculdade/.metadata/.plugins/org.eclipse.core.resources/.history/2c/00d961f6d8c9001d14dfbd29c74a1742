package aulaDia230323;

import java.util.Scanner;

public class Alg64 {
	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		
		System.out.print("Digite um temperatura em Grau Celsius: ");
		int c = ler.nextInt();
		
		double f = (c * 9/5) + 32;
		
		System.out.println(f);
		ler.close();
	}
}
